<Global>
  <solution>
    <k>jinn</k>
    <language>C++</language>
    <configurations>Release-Static</configurations>
    <platforms>x64</platforms>
    <kind>StaticLib</kind>
    <location>src</location>
    <flags>
      <v>AllWarnings</v>
      <v>FatalWarnings</v>
      <v>Symbols</v>
    </flags>
    <includedirs>
      <v>include</v>
      <v>src</v>
    </includedirs>
    <configuration>
      <v>Release</v>
      <kind>SharedLib</kind>
    </configuration>
    <configuration>
      <v>Release-Static</v>
      <kind>StaticLib</kind>
    </configuration>
    <configuration>
      <v>windows</v>
      <defines>WIN32</defines>
    </configuration>
    <configuration>
      <v>gcc</v>
      <flags>Optimize</flags>
      <buildoptions>-fno-strict-aliasing</buildoptions>
      <buildoptions_cpp>-std=c++11</buildoptions_cpp>
      <linkoptions>
        <v>-z defs</v>
        <v>-Wl,-rpath-link,../../bin-linux</v>
      </linkoptions>
    </configuration>
    <configuration>
      <v>vs2019</v>
      <buildoptions>
        <v>/MD</v>
        <v>/EHc</v>
        <v>/EHs</v>
        <v>/Oi</v>
        <v>/Ot</v>
        <v>/W3</v>
      </buildoptions>
      <flags>OptimizeSpeed</flags>
      <linkoptions>/NODEFAULTLIB:OLDNAMES.LIB</linkoptions>
      <disablewarnings>
        <v>4710</v>
        <v>4711</v>
      </disablewarnings>
      <flags>
        <v>NoManifest</v>
        <v>NoIncrementalLink</v>
      </flags>
    </configuration>
    <configuration>
      <v>x64</v>
      <targetdir>bin</targetdir>
      <libdirs>lib</libdirs>
    </configuration>
    <configuration>
      <v>x32</v>
      <targetdir>bin32</targetdir>
      <libdirs>lib32</libdirs>
    </configuration>
    <project>
      <k>futil</k>
      <group>libs</group>
      <location>src/futil</location>
      <objdir>src/futil/.objs</objdir>
      <files>
        <v>src/futil/**.h</v>
        <v>src/futil/**.cpp</v>
        <v>src/futil/**.c</v>
        <v>src/futil/**.lua</v>
      </files>
      <targetname>futil-s</targetname>
      <configuration>
        <v>vs2019</v>
        <v>Release</v>
        <defines>FUTIL_DECLSPEC=__declspec(dllexport)</defines>
        <implibname>futil</implibname>
      </configuration>
      <configuration>
        <v>linux</v>
        <linkoptions>
          <v>-static</v>
          <v>-static-libstdc++</v>
        </linkoptions>
      </configuration>
      <configuration>
        <v>x32</v>
        <v>vs2019</v>
        <v>Release</v>
        <implibdir>lib32</implibdir>
      </configuration>
      <configuration>
        <v>x64</v>
        <v>vs2019</v>
        <implibdir>lib</implibdir>
      </configuration>
      <configuration>
        <v></v>
      </configuration>
      <configuration>
        <v></v>
      </configuration>
      <configuration>
        <v>vs2019</v>
        <pdbdir>$(TargetDir)</pdbdir>
      </configuration>
    </project>
    <project>
      <k>jinn</k>
      <group>apps</group>
      <kind>ConsoleApp</kind>
      <location>src/jinn</location>
      <objdir>src/jinn/.objs</objdir>
      <files>
        <v>src/jinn/**.h</v>
        <v>src/jinn/**.cpp</v>
        <v>src/jinn/**.c</v>
        <v>src/jinn/**.lua</v>
        <v>premake4.lua</v>
        <v>ggen.sh</v>
        <v>jgen.sh</v>
        <v>lua/**.lua</v>
        <v>jinnfile.*</v>
        <v>package*.sh</v>
        <v>deps.sh</v>
        <v>build-win.bat</v>
        <v>build.sh</v>
        <v>README.md</v>
        <v>LICENSE</v>
      </files>
      <links>futil</links>
      <configuration>
        <v>vs2019</v>
        <v>Release</v>
        <defines>JINN_DECLSPEC=__declspec(dllexport)</defines>
        <implibname>jinn</implibname>
      </configuration>
      <configuration>
        <v>linux</v>
        <linkoptions>
          <v>-static</v>
          <v>-static-libstdc++</v>
        </linkoptions>
      </configuration>
      <configuration>
        <v>x32</v>
        <v>vs2019</v>
        <v>Release</v>
        <implibdir>lib32</implibdir>
      </configuration>
      <configuration>
        <v>x64</v>
        <v>vs2019</v>
        <implibdir>lib</implibdir>
      </configuration>
      <configuration>
        <v></v>
      </configuration>
      <configuration>
        <v></v>
      </configuration>
      <configuration>
        <v>vs2019</v>
        <linkoptions>
          <v>/MANIFESTUAC:NO</v>
          <v>/MANIFEST</v>
        </linkoptions>
      </configuration>
      <configuration>
        <v>vs2019</v>
        <v>Release-Static</v>
      </configuration>
      <configuration>
        <v>linux</v>
        <v>Release-Static</v>
      </configuration>
      <configuration>
        <v>linux</v>
      </configuration>
    </project>
  </solution>
</Global>
